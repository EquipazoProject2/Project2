<form action="" class="view-form">
  <h3>Make your reservation</h3>
  <input type="number" placeholder="Amount of people">
  <input autocomplete="false" list="experiences" name="" placeholder="Choose your experience">
  <datalist id="experiences">
    <option value="Japanese">
    <option value="Arabic">
    <option value="Lebanese">
    <option value="Indian">
    <option value="Peruvian">
  </datalist>
  <label for="appt">Choose a time for your reservation</label>
